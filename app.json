{
  "expo": {
    "name": "Todo list",
    "slug": "ReactNativeOneApp",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android"],
    "packagerOpts": {
      "assetExts": ["ttf"]
    },
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/rn-icon.png",
    "splash": {
      "image": "./assets/rn-splash.png",
      "resizeMode": "cover",
