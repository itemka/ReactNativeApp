    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.itemkadev.todo-list-rn-app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.itemkadev.todo_list_rn_app",
      "versionCode": 1,
      "permissions": []
    }
  }
